  "description": "Generate TypeScript API clients, React Query hooks, and OpenAPI specs from Bruno .bru files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "bruno-api": "./dist/cli/index.js"
  },
  "scripts": {
